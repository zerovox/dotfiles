{
    "editor.pasteAs.enabled": "false",
    "editor.pasteAs.showPasteSelector": "never",
    "typescript.updateImportsOnPaste.enabled": false,
    "workbench.colorTheme": "Andromeda Colorizer",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.sideBar.location": "left",
    "window.title": "${activeEditorMedium}${separator}${rootName}",
    "breadcrumbs.enabled": false,
    "editor.minimap.enabled": false,
    "workbench.editor.showTabs": "none",
    "vscode_custom_css.imports": ["/Volumes/git/github.com/zerovox/dotfiles/vscode/.vscode.css"],
    "workbench.colorCustomizations": {
        "[MoonRise]": {
            "sideBar.background": "#222222",
            "editor.background": "#202020",
            "editor.selectionBackground": "#3e515d",
            "editor.selectionHighlightBorder": "#606060"
        },
        "[RainbowDrops]": {
            "tree.indentGuidesStroke": "#00000000",
            "editor.background": "#202020",
            "editor.lineHighlightBackground": "#3e515d40"
        },
        "[Andromeda Colorizer]": {
            "tree.indentGuidesStroke": "#00000000",
            "editor.lineHighlightBackground": "#3e515d40",
            "input.background": "#3e515d40",
            "statusBar.background": "#3e515d40",
            "statusBarItem.remoteBackground": "#3e515d60",
            "sideBarSectionHeader.background": "#3e515d60",
            "titleBar.activeBackground": "#3e515d60"
        }
    },
    "editor.scrollBeyondLastLine": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "afterDelay",
    "eslint.workingDirectories": [{"pattern": "./packages/*/"}],
    "typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "git.openDiffOnClick": false,
    "workbench.tree.indent": 20,
    "workbench.iconTheme": "vscode-jetbrains-icon-theme",
    "githubPullRequests.queries": [
        {
            "label": "Waiting For My Review",
            "query": "repo:${owner}/${repository} is:open review-requested:${user}"
        },
        {
            "label": "Assigned To Me",
            "query": "repo:${owner}/${repository} is:open assignee:${user}"
        },
        {
            "label": "Created By Me",
            "query": "repo:${owner}/${repository} is:open author:${user}"
        },
        {
            "label": "Mentioned Me",
            "query": "repo:${owner}/${repository} is:open mentions:${user}"
        }
    ],
    "git.autofetch": true,
    "workbench.activityBar.location": "top",
    "clipboard-manager.preview": false,
    "clipboard-manager.saveTo": "/Users/tslatcher/.vscode/clipboard-manager-clips.json",
    "clipboard-manager.snippet.enabled": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "continue.telemetryEnabled": false,
    "continue.enableTabAutocomplete": false,
    "diffEditor.codeLens": true,
    "debug.onTaskErrors": "debugAnyway",
    "editor.codeActionsOnSave": {},
    "typescript.tsserver.maxTsServerMemory": 6144
}
